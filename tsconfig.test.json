{
    "extends": "./tsconfig.app.json",
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "node",
        "allowImportingTsExtensions": false,
        "verbatimModuleSyntax": false,
        "noEmit": false,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*", "src/**/*.test.tsx", "src/**/*.test.ts"]
}
